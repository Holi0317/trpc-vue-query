{
  "name": "@tvq/h3-adapter",
  "version": "0.1.0",
  "description": "H3 adapter for trpc",
  "license": "MIT",
  "keywords": [
    "trpc",
    "h3",
    "nitro",
    "nuxt"
  ],
  "author": "Hollis Wu",
  "repository": "Holi0317/tvq",
  "bugs": "https://github.com/Holi0317/tvq/issues",
  "homepage": "https://holi0317.github.io/tvq",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "devDependencies": {
    "@trpc/client": "^11.0.0-rc",
    "@types/node": "^20.11.28",
    "get-port": "^7.0.0",
    "typescript": "^5.4.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.4.0",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "@trpc/server": "^11.0.0-rc",
    "h3": "^1.11.1"
  }
}
